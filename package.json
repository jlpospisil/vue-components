{
  "name": "@cdpjs/vue-components",
  "version": "0.6.0",
  "description": "Reusable Vue.js components",
  "main": "./dist/@cdpjs/vue-components.common.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vue-cli-service build --target wc-async --name cdpjs-components ./src/components/index.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "./node_modules/.bin/eslint src/. .storybook/.",
    "lint:fix": "./node_modules/.bin/eslint src/. .storybook/. --fix",
    "test:unit": "./node_modules/.bin/jest"
  },
  "author": "Josh Pospisil",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-notes": "^4.1.11",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/addon-storysource": "^4.1.11",
    "@storybook/vue": "^4.1.11",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "color-converter": "^1.4.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^24.0.0",
    "storybook-addon-vue-info": "^0.7.1",
    "vue-jest": "^3.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "^4.2.1",
    "google-maps-api-loader": "^1.1.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "node-sass-utils": "^1.1.2",
    "popper.js": "^1.14.6",
    "sass-loader": "^7.1.0",
    "toastr": "^2.1.4",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.1",
    "vue-loading-spinner": "^1.0.11",
    "vue-multiselect": "^2.1.3",
    "vue-template-compiler": "^2.5.22"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
