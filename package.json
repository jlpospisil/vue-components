{
  "name": "@cdpjs/vue-components",
  "version": "0.8.1",
  "description": "Reusable Vue.js components",
  "main": "src/components/index.js",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "npm run lint:js & npm run lint:style",
    "lint:js": "./node_modules/.bin/eslint src/. .storybook/.",
    "lint:js:fix": "./node_modules/.bin/eslint src/. .storybook/. --fix",
    "lint:style": "stylelint src/**/*.vue",
    "test:unit": "./node_modules/.bin/jest"
  },
  "author": "Josh Pospisil",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-knobs": "^4.1.13",
    "@storybook/addon-notes": "^4.1.13",
    "@storybook/addon-options": "^4.1.13",
    "@storybook/addon-storysource": "^4.1.13",
    "@storybook/vue": "^4.1.13",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "color-converter": "^1.4.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "storybook-addon-vue-info": "^0.7.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-processor-html": "^1.0.0",
    "vue-jest": "^3.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.2.3",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "^4.3.1",
    "google-maps-api-loader": "^1.1.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "node-sass-utils": "^1.1.2",
    "popper.js": "^1.14.7",
    "sass-loader": "^7.1.0",
    "splitpanes": "^1.11.3",
    "toastr": "^2.1.4",
    "vue": "^2.6.7",
    "vue-loader": "^15.6.4",
    "vue-loading-spinner": "^1.0.11",
    "vue-multiselect": "^2.1.3",
    "vue-template-compiler": "^2.6.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
