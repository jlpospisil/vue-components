{
  "name": "@cdpjs/vue-components",
  "version": "0.1.0",
  "description": "Reusable Vue.js components",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "./node_modules/.bin/eslint src/. .storybook/.",
    "lint:fix": "./node_modules/.bin/eslint src/. .storybook/. --fix",
    "test:unit": "./node_modules/.bin/jest"
  },
  "author": "Josh Pospisil",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^4.7.1",
    "jest": "^23.6.0",
    "vue-jest": "^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "@storybook/addon-actions": "^4.0.7",
    "@storybook/addon-notes": "^4.0.7",
    "@storybook/addon-options": "^4.0.7",
    "@storybook/addon-storysource": "^4.0.7",
    "@storybook/vue": "^4.0.7",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "popper.js": "^1.14.5",
    "sass-loader": "^7.1.0",
    "storybook-addon-vue-info": "^0.7.0",
    "toastr": "^2.1.4",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
